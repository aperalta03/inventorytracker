{
    "build": {
      "env": {
        "NODE_ENV": "production"
      }
    },
    "functions": {
      "api/**/*.js": {
        "memory": 1024,
        "maxDuration": 10
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "buildCommand": "npm install --legacy-peer-deps && npm run build"
  }
  